# vim: noexpandtab softtabstop=0 shiftwidth=8
[alias]
	br = branch
	cl = clone
	co = checkout
	con = config
	st = status -sb
[core]
	preloadindex = true
	fscache = true
	excludesfile = ~/.gitignore
[diff]
	colorMoved = true
[diff "sqlite3"]
	binary = true
	textconv = "echo .dump | sqlite3"
[user]
	name = "equal-l2"
	email = "eng.equall2@gmail.com"
[hub]
	protocol = https
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
